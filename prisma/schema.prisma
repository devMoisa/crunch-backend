generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  name      String
  surname   String
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
  deleted   Boolean  @default(false)
  updatedAt DateTime @updatedAt
}
